<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <data noupdate="0">
       <record model="res.groups" id="group_academy_admin">
           <field name="name">Academy admin</field>
       </record>
       <record model="ir.rule" id="only_active_modify">
           <field name="name">Only active courses can be modified</field>
           <field name="perm_read" eval="1"></field>
           <field name="perm_write" eval="1"></field>
           <field name="perm_create" eval="1"></field>
           <field name="perm_unlink" eval="1"></field>
           <field name="domain_force">[('active','=',True)]</field>
           <field name="model_id" ref="model_academy_course"></field>
       </record>
  </data>
</odoo>    